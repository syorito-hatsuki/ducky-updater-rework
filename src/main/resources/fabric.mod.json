{
  "schemaVersion": 1,
  "id": "ducky-updater-rework",
  "version": "${version}",
  "name": "Ducky Updater: ReWork",
  "authors": [
    "Syorito Hatsuki"
  ],
  "contact": {
    "homepage": "https://syorito-hatsuki.dev/"
  },
  "icon": "assets/duckyupdaterrework/icon.png",
  "license": "MIT Licence",
  "environment": "server",
  "mixins": [
    "duckyupdaterrework.mixins.json"
  ],
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.syoritohatsuki.duckyupdaterrework.DuckyUpdaterReWork"
